{
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "include": [
    "**/*.ts",
    "**/*.vue"
  ],
  "exclude": [
    "node_modules"
  ],
  "compilerOptions": {
    "lib": [
      "ES2017",
      "DOM"
    ],
    "baseUrl": ".",
    "allowJs": true,
    "paths": {
      "@/*": [
        "./src/*"
      ]
    }
  }
}
